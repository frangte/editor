{
  "name": "editor",
  "version": "0.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --config website/webpack.config.js",
    "build": "rm -r -f dist/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module es2020   --outDir dist/esm && copyfiles -u 1 src/**/*.css src/**/*.svg dist/cjs",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs && copyfiles -u 1 src/**/*.css src/**/*.svg dist/esm",
    "prepare": "npm run build"
  },
  "dependencies": {
    "codemirror": "^5.58.1",
    "copy-to-clipboard": "^3.3.1",
    "katex": "^0.12.0",
    "prosemirror-commands": "^1.1.4",
    "prosemirror-dropcursor": "^1.3.2",
    "prosemirror-gapcursor": "^1.1.5",
    "prosemirror-history": "^1.1.3",
    "prosemirror-inputrules": "^1.1.3",
    "prosemirror-keymap": "^1.1.4",
    "prosemirror-markdown": "^1.5.0",
    "prosemirror-model": "^1.12.0",
    "prosemirror-schema-list": "^1.1.4",
    "prosemirror-state": "^1.3.3",
    "prosemirror-tables": "^1.1.1",
    "prosemirror-utils": "^0.9.6",
    "prosemirror-view": "^1.16.0"
  },
  "devDependencies": {
    "@types/katex": "^0.11.0",
    "@types/prosemirror-commands": "^1.0.1",
    "@types/prosemirror-dropcursor": "^1.0.0",
    "@types/prosemirror-gapcursor": "^1.0.1",
    "@types/prosemirror-history": "^1.0.1",
    "@types/prosemirror-inputrules": "^1.0.3",
    "@types/prosemirror-keymap": "^1.0.1",
    "@types/prosemirror-markdown": "^1.0.3",
    "@types/prosemirror-model": "^1.7.4",
    "@types/prosemirror-schema-list": "^1.0.1",
    "@types/prosemirror-state": "^1.2.4",
    "@types/prosemirror-view": "^1.15.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "copyfiles": "^2.4.0",
    "css-loader": "^4.3.0",
    "exports-loader": "^1.1.0",
    "file-loader": "^6.1.0",
    "gulp": "^4.0.2",
    "gulp-svg-sprite": "^1.5.0",
    "html-webpack-plugin": "^4.5.0",
    "prosemirror-dev-tools": "^3.0.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-serve": "^3.2.0"
  }
}
